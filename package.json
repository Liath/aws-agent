{
  "name": "aws-agent",
  "version": "0.0.5",
  "description": "Chrome extension enables access to AWS resources",
  "main": "n",
  "scripts": {
    "test": "mocha",
    "build": "gulp",
    "package": "rm dist.zip &> /dev/null && rm -R dist && gulp && 7z a dist.zip dist/*",
    "preversion": "npm t",
    "version": "json -I -f lib/manifest.json -e \"this.version=\\\"$(json -f package.json version)\\\"\" && git add lib/manifest.json"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "aws",
    "sigv4",
    "chrome-extension"
  ],
  "author": "John Jones <john.jones@unifilabs.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "aliasify": "^2.1.0",
    "assert": "^1.4.1",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-browserify": "^0.5.1",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "json": "^9.0.6",
    "mocha": "^3.3.0",
    "pbkdf2": "^3.0.12",
    "pump": "^1.0.2",
    "querystring-browser": "^1.0.4",
    "text-encoding": "^0.6.4",
    "url": "^0.11.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "aws4": "*"
  },
  "aliasify": {
    "aliases": {
      "querystring": "querystring-browser"
    }
  }
}
